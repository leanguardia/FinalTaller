<h1>Listing goal_ranges</h1>

<table>
  <thead>
    <tr>
      <th>Goal</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_ranges.each do |goal_range| %>
      <tr>
        <td><%= goal_range.goal %></td>
        <td><%= goal_range.start_time %></td>
        <td><%= goal_range.end_time %></td>
        <td><%= link_to 'Show', goal_range %></td>
        <td><%= link_to 'Edit', edit_goal_range_path(goal_range) %></td>
        <td><%= link_to 'Destroy', goal_range, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal range', new_goal_range_path %>
